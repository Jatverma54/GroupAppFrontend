{
  "expo": {
    "name": "GroupHelpMe",
    "description":"Group App",
    "slug": "GroupHelpMe", 
    "sdkVersion":"39.0.0" ,
    "privacy":"unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/circle-312343.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "icon": "./assets/icon.png",
      "androidMode": "default",
  "androidCollapsedTitle": "Updates from Colab.notes",
  "iosDisplayInForeground": true
    },
    "android": {
      "package":"com.grouphelpme.grouphelpmeApp",
      "googleServicesFile": "./googles.json",
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1558609691925120~2018624642" 
      },
     "adaptiveIcon":{
       "foregroundImage":"./assets/icon.png",
       "backgroundColor":"#171717"
     },
     "permissions":[
       "CAMERA",
       "NOTIFICATIONS",
       "USER_FACING_NOTIFICATIONS",
       "AUDIO_RECORDING",
       "CAMERA_ROLL",
       "CALENDAR",
       "MOTION"
    ]
    },
    "androidStatusBar": {
      "backgroundColor": "#086A87",
      "translucent": true
    }
   
  }
}
